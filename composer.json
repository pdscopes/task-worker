{
    "name": "madesimple/task-worker",
    "description": "Generic background task worker library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Scopes",
            "email": "peter.scopes@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "psr/simple-cache": "~1.0.0",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "MadeSimple\\TaskWorker\\": "src/"
        }
    },
    "suggest": {
        "cache/cache": "Library of all the php-cache adapters",
        "symfony/console": "Symfony console commands more robust CLI",
        "vlucas/phpdotenv": "Simple dot env (`.env`) loader",
        "php-amqplib/php-amqplib": "Allow RabbitMQ Queue",
        "predis/predis": "Allow Redis Queue"
    },
    "require-dev": {
        "cache/cache": "~1.0",
        "monolog/monolog": "^1.23",
        "symfony/console": "~3.2",
        "vlucas/phpdotenv": "^2.4",
        "php-amqplib/php-amqplib": "^2.7",
        "predis/predis": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "MadeSimple\\TaskWorker\\Example\\": "examples/",
            "MadeSimple\\TaskWorker\\Test\\": "tests/"
        }
    }
}
